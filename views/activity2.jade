extends layout
block headcontent
  link(href='complete-admin/resource/calendar/css/fullcalendar.css', rel='stylesheet', type='text/css')
  script(src='complete-admin/resource/calendar/js/fullcalendar.min.js', type='text/javascript')
  script(src='complete-admin/resource/calendar/js/calendar.js', type='text/javascript')
  script(src='complete-admin/resource/calendar/js/jquery.custom.min.js', type='text/javascript')
block content
  .container-fluid
    .row-fluid
      .well
        .row-fluid
          #calendar
        .row-fluid
          h2(style='border-bottom:solid 2px;') Workout Programs
    .row-fluid
      .span6
        .accordion#accordion2
          .accordion-group
            .accordion-heading
              a.accordion-toggle.btn(data-toggle="collapse", href="#collapseChart3", data-parent="#accordion2", style="text-align: left; text-decoration: none;")
                i.icon-bar-chart
                |  Activity
            .accordion-body.collapse.in#collapseChart3
              .accordion-inner
                #column
    .row-fluid
        .well
          #chart
            .highcharts-container#highcharts-0

  script(src="complete-admin/resource/chart/chart-column.js", type="text/javascript")
  script(src="complete-admin/resource/chart/chart.js", type="text/javascript")
  script(src="complete-admin/resource/chart/highcharts.js", type="text/javascript")