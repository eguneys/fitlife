extends layout
block headcontent
  link(href='complete-admin/resource/calendar/css/fullcalendar.css', rel='stylesheet', type='text/css')
  script(src='complete-admin/resource/calendar/js/fullcalendar.min.js', type='text/javascript')
  script(src='complete-admin/resource/calendar/js/calendar.js', type='text/javascript')
  script(src='complete-admin/resource/calendar/js/jquery.custom.min.js', type='text/javascript')
block content
  .jumbotron
    .row
          .panel.panel-primary
            .panel-heading.bs-panel-heading
              button.btn.btn-primary(type='button', data-toggle='collapse', data-target='.bs-collapse-calendar', style='width: 100%;')
                i.icon-calendar
                |  Workout Calendar
            .bs-collapse-calendar.in
              #calendar
    .row
      .panel.panel-primary
        .panel-heading.bs-panel-heading
          button.btn.btn-primary(type='button', data-toggle='collapse', data-target='.bs-collapse-program', style='width: 100%;')
            i.icon-th-list
            |  Workout Programs
        .bs-collapse-program.in
          
    .row
      .panel.panel-primary
        .panel-heading.bs-panel-heading
          button.btn.btn-primary(type='button', data-toggle='collapse', data-target='.bs-collapse-activity', style='width: 100%;')
            i.icon-bar-chart
            |  Activity
        .bs-collapse-activity.in
          .row
              #column
          .row
              #chart
                .highcharts-container#highcharts-0

  script(src="complete-admin/resource/chart/chart-column.js", type="text/javascript")
  script(src="complete-admin/resource/chart/chart.js", type="text/javascript")
  script(src="complete-admin/resource/chart/highcharts.js", type="text/javascript")